{
  "name": "cypresstesting",
  "version": "1.0.0",
  "description": "testAutomationCypress",
  "main": "index.js",
  "scripts": {
    "openEditor": "npx cypress open",
    "cypress:open:dev": "CYPRESS_ENV=dev npx cypress open",
    "cypress:open:prod": "CYPRESS_ENV=prod npx cypress open",
    "getInfo": "npx cypress info",
    "runTest": "npx cypress run",
    "chrome": "npx cypress run --browser chrome", 
    "firefox": "npx cypress run --browser firefox", 
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "clean:screens":"rm -R -f cypress/screenshots && rm -R -f cypress/videos",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "merge-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports", 
    "report": "npm run combine-reports && npm run merge-report",
    "full:cleanup": "npm run clean:screens && npm run clean:reports"


  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rszatkowski/cypressTesting.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rszatkowski/cypressTesting/issues"
  },
  "homepage": "https://github.com/rszatkowski/cypressTesting#readme",
  
  
  "devDependencies": {
    "cypress": "^4.6.0",
    "cypress-multi-reporters": "^1.4.0",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "mochawesome-report-generator": "^5.1.0"
  },
  "dependencies": {
    "dev": "^0.1.3",
    "mocha": "^7.2.0"
  }
}
